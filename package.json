{
  "name": "elexams",
  "version": "0.3.0",
  "description": "A frontend for obcode/plexams server",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dev": "rollup -cw",
    "build": "rollup -c",
    "postinstall": "electron-builder install-app-deps",
    "test": "true",
    "release": "build --publish=never",
    "release:all": "build -mwl -p always",
    "release:win": "build --win -p always",
    "release:mac": "build --mac -p always",
    "release:linux": "build --linux -p always"
  },
  "repository": "https://github.com/obcode/elexams",
  "author": "Oliver Braun",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "rollup": "^1.27.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-typescript": "^1.0.1",
    "svelte": "^3.15.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "chokidar": "^3.3.0",
    "cross-env": "^6.0.3"
  }
}
